    <string name="generating_start_animation_text3" comment="'Generating animation' screen - text"><![CDATA[Saving your<br/>encrypted keys to a<br/><font color="#064dd6">local secure vault</font><br/>on this device]]></string>
    <string name="generating_start_animation_text4" comment="'Generating animation' screen - text"><![CDATA[<font color="#064dd6">All done!</font>]]></string>
