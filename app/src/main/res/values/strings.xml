    <string name="pick_password_description" comment="'Pick password' screen - description part 1">So no one else but you can unlock your wallet.</string>
    <string name="pick_password_description2" comment="'Pick password' screen - description part 2">Best passwords are long and contain letters, numbers and special characters.</string>
